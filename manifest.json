{
	"name": "Seneca Blackboard Extension",
	"version": "0.0.0",
	"description": "Changes up the GUI of Seneca Blackboard to be more user friendly",
	"homepage_url": "https://github.com/yevseytsev/SenecaBlackboardExtension",
	"manifest_version": 2,
	"permissions": [
		"https://my.senecacollege.ca/*",
		"https://studenterp.senecacollege.ca/*"
	],
	"content_scripts": [
		{
			"matches": [
				"https://my.senecacollege.ca/*",
				"https://studenterp.senecacollege.ca/*"
            ],
			"js": ["/src/homepage-guest.js", "/src/addEmailLinkExtension/contentScript.js", "/src/studentcenterpage.js"],
			"css": ["src/dark-theme.css"]
		}
	],
	"background": {
		"scripts": ["src/homepage-guest.js"]
	},
	"browser_action": {
		"default_icon": "./images/icon128.png"
	},
  
			
		
	"icons": {
		"128": "./images/icon128.png",
		"48": "./images/icon48.png",
		"32": "./images/icon32.png",
		"16": "./images/icon16.png"
	}
}